getRandomInt = function(min, max) {
  return Math.floor(Math.random() * (max - min)) + min;
}

function integerDivision(x,y){
	return (x-x%y)/y;
}

var NOD = {
	generate: function(){
		a = getRandomInt(4, 1000);
		b = getRandomInt(4, 1000);
		if (b>a) b = [a, a=b][0
		result = {up: [a,b], down: []}
		while(b != 0){
			div = integerDivision(a,b);
			mod = a%b;
			result.down.push(div);
			result.up.push(mod);
			a = b; b = mod;
		}
		return result;
	}
}

module.exports.getRandomInt = getRandomInt;
module.exports.NOD = NOD;
